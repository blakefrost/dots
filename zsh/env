# Setup MacPorts
export PATH=/opt/local/bin:/opt/local/sbin:$PATH

# MySQL Path
export PATH=/usr/local/mysql/bin:$PATH

# Add current directory & User bin to end of path.
export PATH=$PATH:~/bin:.

# Include gem directory in path and setup RVM.
export PATH=`gem env gemdir`:$PATH
[[ -s $HOME/.rvm/scripts/rvm ]] && source $HOME/.rvm/scripts/rvm

# Turn on git bash completion
#if [ -f ~/bin/bash_completion/git ]; then
#  . ~/bin/bash_completion/git
#fi


