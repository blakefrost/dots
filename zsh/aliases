export ALAISES_PATH=~/bin/dotfiles/zsh/aliases

alias tm='open *.tmproj'

alias ..='cd ..'
alias cd..='cd ..'
alias ea='vim $ALAISES_PATH; reload'
alias reload='source $ALAISES_PATH'

alias l=ls\ -l
alias c=clear
alias cl=c\;l

# GIT
alias g=git\ status
alias ga=git\ add
alias gco=git\ checkout
alias gc=git\ commit
alias gb=git\ branch
alias gd=git\ diff

alias cdd=cd\ -
alias j='mvim ~/.notes/blake/journal'

# Not Working
#rtw () {
#        sed -i 's/[[:space:]]*$//' "$1"
#}

ff () {
# ff x find file named x
find . -name $1 -print
}

files () {
# files x => list files in x
find $1 -type f -print
}




